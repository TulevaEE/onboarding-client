{
  "name": "onboarding-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sentry/browser": "^5.6.3",
    "array.prototype.find": "^2.1.0",
    "axios": "^0.21.1",
    "bootstrap": "4.3.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.9.1",
    "downloadjs": "^1.4.7",
    "express": "^4.17.1",
    "express-http-proxy": "^1.2.0",
    "font-awesome": "^4.7.0",
    "history": "^4.9.0",
    "hwcrypto-js": "git://github.com/ErkoRisthein/hwcrypto.js.git#0.0.14",
    "lodash": "^4.17.20",
    "mixpanel-browser": "^2.29.1",
    "moment": "^2.22.2",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.3",
    "promise": "^8.0.3",
    "prop-types": "^15.7.2",
    "qs": "^6.8.0",
    "react": "^17.0.1",
    "react-app-polyfill": "^1.0.2",
    "react-dom": "^17.0.1",
    "react-facebook": "^8.1.4",
    "react-ga": "^3.3.0",
    "react-global-configuration": "^1.4.1",
    "react-mixpanel": "0.0.11",
    "react-query": "^3.12.1",
    "react-redux": "^7.1.2",
    "react-router-dom": "^5.1.0",
    "react-scripts": "^4.0.2",
    "react-tooltip": "^4.2.15",
    "redux": "^4.0.4",
    "redux-form": "^8.3.7",
    "redux-thunk": "^2.3.0",
    "retranslate": "^1.2.0",
    "typescript": "^4.2.3",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "start": "NODE_ENV=production node server",
    "prestart": "npm run build-onboarding",
    "build-onboarding": "APP_NAME=onboarding npm run build-app",
    "build-signup": "APP_NAME=signup npm run build-app",
    "build-login": "APP_NAME=login npm run build-app",
    "build-app": "npm-run-all backup-index create-index-for-app build restore-index",
    "backup-index": "mv src/index.js src/index.js.backup",
    "create-index-for-app": "echo \"import './entries/$APP_NAME';\" > src/index.js",
    "restore-index": "mv src/index.js.backup src/index.js",
    "build": "react-scripts build",
    "develop": "react-scripts start",
    "develop-production": "HTTPS=true HOST=local.tuleva.ee npm run develop",
    "develop-production-win": "set HTTPS=true&&set HOST=local.tuleva.ee&&npm run develop",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx src/",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "format": "eslint --ext .js,.ts,.jsx,.tsx src/ --fix"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@transferwise/eslint-config": "^6.1.0",
    "@types/classnames": "^2.2.11",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.3",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.1",
    "@types/react-global-configuration": "^1.3.1",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^5.1.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "enzyme": "^3.11.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "msw": "^0.27.1",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "redux-mock-store": "^1.5.3"
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "text"
    ]
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "proxy": "http://localhost:9000",
  "//": "https://onboarding-service.tuleva.ee",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
